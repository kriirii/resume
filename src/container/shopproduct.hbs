<main class="p-3">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="p-2">
                    <div class="nav justify-content-end">
                        <h1 class="me-auto">{{layout}}</h1>
                        {{#each navigation.links}}
                        <div class="nav-item">
                            <a href="{{this.href}}" class="nav-link">{{this.text}}</a>
                        </div>
                        {{/each}}
                        <div class="nav-link disabled">{{navigation.Logo}}</div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="relax">
        <br>
        <div class="display-3">{{relaxion.title}}</div>
        <br>
    </div>
    <div class="row g-4">
        {{#each relaxion.cards}}
        <div class="col-6">
            <div class="card text-bg-secondary">
                <div class="card-header">{{header}}</div>
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p>{{description}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>










</main>