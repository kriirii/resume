<main>
    <div class="list-group-item">
        {{#each navigation.links}}
        <a href="{{href}}" class="btn btn-link">{{text}}</a>
        {{/each}}
        <a href="{{navigation.button.href}}" class="btn btn-primary">{{navigation.button.text}}</a>
    </div>
    <hr>
    <br>
    <div class="main-heading">
        <h1>{{header.title}}</h1>
        <p>{{header.description}}</p>
        <a href="{{header.button.link}}" class="btn btn-primary">{{header.button.text}}</a>
    </div>
    <br>
    <hr>
    <div class="city">
        <br>
        <h2>{{city.title}}</h2>
        <br>
    </div>
    <div class="row g-2">
        {{#each city.cards}}
        <div class="col-6">
            <div class="card">
                <img class="card-img-top" src="{{picture.src}}" alt="{{picture.alt}}">
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p>{{description}}</p>
                    <a href="{{button.href}}" class="btn btn-primary">{{button.text}}</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    <br>
    <hr>
    <br>
    <h2>{{winner.title}}</h2>
    <br>

    <table class="table">
        <thead>
            <tr>
                {{#each winner.table.head}}
                <th>
                    {{this}}
                </th>
                {{/each}}
            </tr>
        </thead>
        <tbody>
            {{#each winner.table.user}}
            <tr>
                <th>{{this.place}}</th>
                <td>{{this.name}}</td>
                <td>{{this.surname}}</td>
                <td>{{this.nickname}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <br>
    <hr>
    <br>
    <h2>{{gallery.title}}</h2>
    <br>
    <div class="row g">
        {{#each gallery.cards}}
        <div class="col-4">
            <ul class="list-group">
                <li class="list-group-item text-bg-secondary">
                    <div class="card-header">
                        <p>{{name}}</p>
                    </div>
                </li>
                {{#each items}}
                <li class="list-group-item">
                    <p>{{this}}</p>
                </li>
                {{/each}}
            </ul>
        </div>
        {{/each}}
    </div>
    <br>
    <hr>
    <br>
    <div class="relax">
        <br>
        <div class="display-3">{{relaxion.title}}</div>
        <br>
    </div>
    <div class="row g-4">
        {{#each relaxion.cards}}
        <div class="col-6">
            <div class="card text-bg-secondary">
                <div class="card-header">{{header}}</div>
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p>{{description}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    <br>
    <hr>
    <br>
    <div class="row g-4">
        {{#each rest.cards}}
        <div class="col-6">
            <div class="card">
                <div class="card-header text-center">{{header}}</div>
                <div class="card-body text-center">
                    <h6 class="card-title">{{title}}</h6>
                    <p>{{description}}</p>
                    <button class="btn btn-primary">{{button}}</button>
                </div>
                <div class="card-footer text-center">{{footer}}</div>
            </div>
        </div>
        {{/each}}
    </div>
    <br>
    <hr>
    <br>
    <div class="Proposition">
        <br>
        <div class="display-2">{{proposition.title}}</div>
        <br>
    </div>
    <div class="row g-4">
        {{#each proposition.card}}
        <div class="col-6">
            <div class="card text-bg-warning">
                <div class="card-header">{{header}}</div>
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p>{{description}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    <br>
    <hr>

    <div class="footer">
        <br>
        <h5>{{footer.title}}</h5>
        <br>
    </div>

    <div class="row g-12">
        <div class="col-4">
            {{#each footer.navigation}}
            <div><a href="{{href}}">{{text}}</a></div>
            {{/each}}
        </div>


        <div class="col-4">
            {{#each footer.socialMedia}}
            <div><a href="{{link}}" class="btn btn-outline-dark">{{name}}</a></div>
            {{/each}}
        </div>

        <div class="col-4">
            {{#each footer.partners}}
            <div><a href="{{link}}" class="btn btn-outline-info">{{name}}</a></div>
            {{/each}}
        </div>
    </div>

</main>






</main>