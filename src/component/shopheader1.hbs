<div class="p-3">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body p-2">
                    <h1 class="me-auto">{{navigation.Logo}}</h1>
                    <div class="nav">
                        {{#each navigation.links}}
                        <div class="nav-item">
                            <a href="{{this.href}}" class="nav-link{{#if @last}} disabled{{/if}}">{{this.text}}</a>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>